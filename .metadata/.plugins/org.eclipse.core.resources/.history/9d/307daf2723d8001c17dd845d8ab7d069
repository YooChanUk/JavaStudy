package kunsan.psback0.lecture04;

public class Researcher extends Person
{
	String employeeNumber, dept;
	
	public Researcher(String citizenNumber, String name, int birthYear,
					String employeeNumber, String dept) {
		super(citizenNumber, name, birthYear);
		// TODO Auto-generated constructor stub
		
		this.employeeNumber = employeeNumber;
		this.dept = dept;
	}

	

	public String getEmployeeNumber() 
	{
		return employeeNumber;
	}

	public void setEmployeeNumber(String employeeNumber) 
	{
		this.employeeNumber = employeeNumber;
	}

	public String getDept() 
	{
		return dept;
	}

	public void setDept(String dept) 
	{
		this.dept = dept;
	}

	@Override
	public String toString() 
	{
		return "Researcher [employeeNumber=" + employeeNumber + ", dept=" + dept + ", citizenNumber=" + citizenNumber
				+ ", name=" + name + ", birthYear=" + birthYear + "]";
	}


	
	

}
