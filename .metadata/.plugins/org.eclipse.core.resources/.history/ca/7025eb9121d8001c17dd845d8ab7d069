package kunsan.psback0.lecture04;

public class Professor extends Researcher
{
	String position;
	
	public Professor(String citizenNumber, String name, int birthYear) 
	{
		super(citizenNumber, name, birthYear);
		// TODO Auto-generated constructor stub
	}

	

	public String getPosition() 
	{
		return position;
	}

	public void setPosition(String position) 
	{
		this.position = position;
	}

	@Override
	public String toString()
	{
		return "Professor [position=" + position + "]";
	}
	
	
}
