package MidFixStudent;

public class Student 
{
	String Name, Adress;
	int Stdnum, Schoolyear, birthday;
		
	public Student(String Name, String Adress, int Stdnum, int Schoolyear, int birthday)
	{
		this.Name = Name;
		this.Adress = Adress;
		this.Stdnum = Stdnum;
		this.Schoolyear = Schoolyear;
		this.birthday = birthday;
	}

	
		
	public void AllShow()
	{
		System.out.println(this.Stdnum + " " + this.Name + " " + this.Adress + 
								" " + this.Schoolyear + " " + this.birthday);
	}

}
